{
  "name": "mew151.net",
  "version": "1.0.0",
  "description": "Source for static side of mew151.net",
  "main": "index.js",
  "scripts": {
    "portfolio-prepare": "rm -f src/portfolio/*.template",
    "mew151-prepare": "rm -r src/portfolio",
    "neocities-prepare": "rm -r src/blog",
    "rss": "feedmake -o src/rss.xml .",
    "build": "gulp",
    "build:watch": "gulp watch",
    "dev-server": "http-server src -o",
    "test-dev-server": "http-server src --port 8081",
    "start": "npm-run-all --parallel build:watch dev-server",
    "test": "jest"
  },
  "author": "Natalie Martin",
  "dependencies": {
    "@tsconfig/node16": "^1.0.3",
    "fast-levenshtein": "^3.0.0",
    "feedmake": "github:meisekimiu/feedmake",
    "sass": "^1.54.5",
    "snarkdown": "^2.0.0",
    "typescript": "^4.7.4",
    "uglify-js": "^3.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@types/fast-levenshtein": "^0.0.2",
    "@types/jest": "^29.5.2",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "browserify": "^17.0.0",
    "dotenv": "^16.0.3",
    "expect-puppeteer": "^9.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-imagemin": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^5.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "http-server": "^14.1.1",
    "jest": "^29.5.0",
    "jest-puppeteer": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^20.7.1",
    "ts-jest": "^29.1.0"
  }
}
